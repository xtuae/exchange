{
  "functions": {
    "api/save-transaction.js": {
      "maxDuration": 10
    },
    "api/webhook.js": {
      "maxDuration": 10
    },
    "api/transaction-status.js": {
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/api/session",
      "destination": "https://instaxchange.com/api/session"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    }
  ]
}
